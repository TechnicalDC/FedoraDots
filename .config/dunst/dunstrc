    #icon = /path/to/icon

[urgency_normal]
    background = "#282c34e0"
	foreground = "#abb2bf"
    timeout = 3
    # Icon for notifications with normal urgency, uncomment to enable
    #icon = /path/to/icon

[urgency_critical]
    background = "#282c34e0"
	foreground = "#e06c75"
    timeout = 10
    # Icon for notifications with critical urgency, uncomment to enable
    #icon = /path/to/icon

