    # Otherwise the "#" and following would be interpreted as a comment.
    background = "#282936"
    # background = "#282936f0"
	foreground = "#e9e9f4"
    timeout = 3
    # Icon for notifications with low urgency, uncomment to enable
    #icon = /path/to/icon

[urgency_normal]
    background = "#282936"
    # background = "#282936f0"
	foreground = "#e9e9f4"
    timeout = 3
